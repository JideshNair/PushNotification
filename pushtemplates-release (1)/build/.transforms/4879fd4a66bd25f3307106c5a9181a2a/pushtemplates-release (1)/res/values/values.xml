<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorAccent">#D81B60</color>
    <color name="colorPrimary">#008577</color>
    <color name="colorPrimaryDark">#00574B</color>
    <color name="gray">#808080</color>
    <color name="white">#FFFFFF</color>
    <dimen name="auto_carousel_padding_horizontal">8dp</dimen>
    <dimen name="auto_carousel_padding_vertical">8dp</dimen>
    <dimen name="big_img_height">144dp</dimen>
    <dimen name="big_img_marginTop">10dp</dimen>
    <dimen name="chronometer_font_size">12sp</dimen>
    <dimen name="five_cta_icon_margin">4dp</dimen>
    <dimen name="flipper_height">144dp</dimen>
    <dimen name="large_icon">36dp</dimen>
    <dimen name="manual_carousel_arrow">30dp</dimen>
    <dimen name="metadata_font_size">10dp</dimen>
    <dimen name="metadata_margin_x">4dp</dimen>
    <dimen name="metadata_small_icon_x">16dp</dimen>
    <dimen name="metadata_small_icon_y">16dp</dimen>
    <dimen name="metadata_title_margin_horizontal">4dp</dimen>
    <dimen name="metadata_title_margin_vertical">4dp</dimen>
    <dimen name="msg_font_size">11sp</dimen>
    <dimen name="padding_horizontal">16dp</dimen>
    <dimen name="padding_vertical">4dp</dimen>
    <dimen name="product_display_button_height">24dp</dimen>
    <dimen name="rating_big_img">96dp</dimen>
    <dimen name="rating_star">20dp</dimen>
    <dimen name="title_font_size">11sp</dimen>
    <string name="app_name">PushTemplates</string>
    <style name="Dialog" parent="android:Theme.Dialog">
        <item name="android:windowBackground">@null</item>
    </style>
    <style name="ManualCarouselArrowFwd" parent="android:Widget.TextView">
        <item name="android:layout_width">@dimen/manual_carousel_arrow</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:src">@drawable/pt_arrow_forward</item>
    </style>
    <style name="ManualCarouselArrowRev" parent="android:Widget.TextView">
        <item name="android:layout_width">@dimen/manual_carousel_arrow</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:src">@drawable/pt_arrow_back</item>
    </style>
    <style name="MetaData" parent="android:Widget.TextView">
        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize">@dimen/metadata_font_size</item>
    </style>
    <style name="PushMessage" parent="android:Widget.TextView">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">@dimen/msg_font_size</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="PushTitle" parent="android:Widget.TextView">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/title_font_size</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>
    <declare-styleable name="FixedAspectRatioFrameLayout">
        <attr format="integer" name="aspectRatioWidth"/>
        <attr format="integer" name="aspectRatioHeight"/> </declare-styleable>
</resources>